{
  "name": "little-backup-box-webapp",
  "version": "1.0.0",
  "description": "Little Backup Box Web GUI - Node.js/React version",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "node --watch server/index.js",
    "dev:client": "vite",
    "dev:mock": "USE_MOCKS=true concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "vite build",
    "build:static": "VITE_USE_MOCK_API=true vite build",
    "preview": "vite preview",
    "start": "node server/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:frontend": "eslint src",
    "lint:backend": "eslint server",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.18.0",
    "axios": "^1.6.2",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "ini": "^4.1.1",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.40.0",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "prettier": "^3.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.2",
    "vite": "^7.3.0"
  }
}
